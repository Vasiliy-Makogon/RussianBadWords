    "authors": [
        {
            "name": "Vasiliy Makogon"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krugozor\\RussianBadWords\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Krugozor\\RussianBadWords\\Installer::postInstall"
        ],
        "post-update-cmd": [
            "Krugozor\\RussianBadWords\\Installer::postInstall"
